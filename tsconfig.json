{
  "compilerOptions": {
    "outDir": "./frontend/",
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@public/*": ["public/*"],
      "@http/*": ["http/*"],
      "@router/*": ["router/*"]
    },
    "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ]
}
